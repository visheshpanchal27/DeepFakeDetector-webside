{
  "name": "deepfake-detector",
  "version": "1.0.0",
  "description": "Full-stack DeepFake detection application",
  "scripts": {
    "setup": "cd backend && py -m pip install -r requirements-simple.txt && cd ../frontend && npm install",
    "start": "concurrently --kill-others \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && py app.py",
    "frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}